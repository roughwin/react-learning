{
  "name": "crm-frontend",
  "description": "Smart CRM Project",
  "private": true,
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git@code.smartstudy.com:o2o/crm-frontend.git"
  },
  "scripts": {
    "start": "npm run dllbuild && NODE_ENV=development webpack-dev-server --progress --hot",
    "build": "COMMIT_ID=$(git log --oneline | head -n 1 | sed 's/ .*//') NODE_ENV=production webpack --config webpack.config.prod.js --progress -p --bail",
    "testbuild": "npm run prebuild && BUILD_TIME=$(date +%s) COMMIT_ID=$(git log --oneline | head -n 1 | sed 's/ .*//') NODE_ENV=testing webpack --config webpack.config.prod.js --progress -p --bail",
    "clean": "rimraf public dist",
    "prebuild": "mkdir -p dist && cp -R src/assets/** dist",
    "copy": "mkdir -p public && cp -R src/assets/** public",
    "dllbuild": "webpack --config dll.webpack.config.js",
    "prestart": "npm run clean && npm run copy",
    "lint": "eslint --ext .js,.jsx",
    "lint:git": "eslint `git diff-index --name-only HEAD | grep -e '.js$' -e '.jsx$'`",
    "test": "jest"
  },
  "pre-commit": [
    "lint:git"
  ],
  "dependencies": {
    "antd": "^2.9.0",
    "driver.js": "^0.3.8",
    "gpu.js": "^1.2.0",
    "lodash": "^4.17.4",
    "mobx": "^3.1.7",
    "mobx-react": "^4.1.3",
    "mobx-utils": "^2.0.1",
    "moment": "^2.18.1",
    "phone": "^1.0.11",
    "prop-types": "^15.5.10",
    "qrcode.react": "^0.7.1",
    "qs": "^6.4.0",
    "react": "^15.6.1",
    "react-dnd": "^2.4.0",
    "react-dnd-html5-backend": "^2.4.1",
    "react-dom": "^15.4.2",
    "react-loading": "^0.0.9",
    "react-router-dom": "^4.0.0",
    "react-transition-group": "^2.2.1",
    "rxjs": "^5.5.6",
    "three": "^0.91.0",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.0",
    "babel-jest": "^21.0.2",
    "babel-loader": "^6.4.1",
    "babel-plugin-dynamic-import-webpack": "^1.0.2",
    "babel-plugin-import": "^1.1.1",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.2.2",
    "babel-preset-react": "^6.23.0",
    "css-loader": "^0.27.3",
    "enzyme": "^2.9.1",
    "eslint": "^3.18.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "hard-source-webpack-plugin": "^0.4.11",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^21.1.0",
    "jsdom": "^11.2.0",
    "less": "^2.7.2",
    "less-loader": "^4.0.2",
    "mobx-react-devtools": "^4.2.11",
    "pre-commit": "^1.2.2",
    "react-dom": "^15.6.1",
    "react-test-renderer": "^15.6.1",
    "regenerator-runtime": "^0.11.0",
    "rimraf": "^2.6.1",
    "shared-worker-loader": "^0.2.0",
    "style-loader": "^0.16.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.3.1",
    "webpack-dev-server": "^2.4.2",
    "worker-loader": "^0.8.1"
  }
}
